<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?bald_background"
    android:weightSum="5"
    tools:ignore="RtlHardcoded,HardcodedText">

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_1"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="1"
        app:layout_constraintBottom_toTopOf="@+id/b_4"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/b_2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_2"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="2"
        app:layout_constraintBottom_toTopOf="@+id/b_5"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toRightOf="@+id/b_1"
        app:layout_constraintRight_toLeftOf="@+id/b_3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_3"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="3"
        app:layout_constraintBottom_toTopOf="@+id/b_6"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toRightOf="@+id/b_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_4"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="4"
        app:layout_constraintBottom_toTopOf="@+id/b_7"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/b_5"
        app:layout_constraintTop_toBottomOf="@+id/b_1"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_5"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="5"
        app:layout_constraintBottom_toTopOf="@+id/b_8"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toRightOf="@+id/b_4"
        app:layout_constraintRight_toLeftOf="@+id/b_6"
        app:layout_constraintTop_toBottomOf="@+id/b_2"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_6"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="6"
        app:layout_constraintBottom_toTopOf="@+id/b_9"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toRightOf="@+id/b_5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_3"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_7"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="7"
        app:layout_constraintBottom_toTopOf="@+id/b_sulamit"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/b_8"
        app:layout_constraintTop_toBottomOf="@+id/b_4"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_8"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="8"
        app:layout_constraintBottom_toTopOf="@+id/b_0"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toRightOf="@+id/b_7"
        app:layout_constraintRight_toLeftOf="@+id/b_9"
        app:layout_constraintTop_toBottomOf="@+id/b_5"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_9"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="9"
        app:layout_constraintBottom_toTopOf="@+id/b_hash"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toRightOf="@+id/b_8"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_6"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_sulamit"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="*"
        app:layout_constraintBottom_toTopOf="@+id/b_clear"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/b_0"
        app:layout_constraintTop_toBottomOf="@+id/b_7"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_0"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/b_call"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toRightOf="@+id/b_sulamit"
        app:layout_constraintRight_toLeftOf="@+id/b_hash"
        app:layout_constraintTop_toBottomOf="@+id/b_8"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldButton
        android:id="@+id/b_hash"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:text="#"
        app:layout_constraintBottom_toTopOf="@+id/b_backspace"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toRightOf="@+id/b_0"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_9"
        app:layout_constraintWidth_percent=".3" />


    <com.bald.uriah.baldphone.views.BaldImageButton
        android:id="@+id/b_clear"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:padding="5dp"
        android:src="@drawable/delete_on_button"
        android:text="@string/clear"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/b_call"
        app:layout_constraintTop_toBottomOf="@+id/b_sulamit"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldImageButton
        android:id="@+id/b_call"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:gravity="center"
        android:padding="10dp"
        android:src="@drawable/phone_on_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toRightOf="@+id/b_clear"
        app:layout_constraintRight_toLeftOf="@+id/b_backspace"
        app:layout_constraintTop_toBottomOf="@+id/b_0"
        app:layout_constraintWidth_percent=".3" />

    <com.bald.uriah.baldphone.views.BaldImageButton
        android:id="@+id/b_backspace"
        style="@style/ButtonsText.Auto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/style_for_buttons"
        android:padding="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.18"
        app:layout_constraintLeft_toRightOf="@+id/b_call"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_hash"
        app:layout_constraintWidth_percent=".3"
        app:srcCompat="@drawable/backspace_on_button" />
</android.support.constraint.ConstraintLayout>